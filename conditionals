public class Main {

    public static void main(String[] args) {
        int sayi = 3;
        if (sayi<20) {
            System.out.println("sayı, 20'den küçük");
        }else if (sayi==20) {
            System.out.println("sayı, 20'ye eşit");
        }else{
            System.out.println("sayı, 20'den büyük");
        }
    }
    
}
